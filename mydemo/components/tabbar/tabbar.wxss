._tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9999;
  width: 100%;
  padding-bottom: 48rpx;
  box-sizing: border-box;
  padding-left: 36rpx;
  padding-right: 36rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: #fff;
  font-size: 26rpx;
  color: #cdcdcd;
  box-shadow: 0px -7rpx 13rpx 0px rgba(193, 185, 204, 0.13);
}
._tabbar .titem {
  text-align: center;
  /* width: 33.3%; */
}
._tabbar .titem image {
  display: block;
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
}

._tabbarTxt{
  font-size: 10px;
}

._tabbar .tCdk {
}

._tabbar .tCdkTxt{
    color: #1296db;
    margin-bottom: 18rpx;
    display: block;
}

._tabbar .tCdkImg{
    transition: .6s;
    transform: scale(1.4);
    margin-bottom: 24rpx;
  }
  image{
    padding: 12rpx;
    display: block;
    background: #fff;
    box-sizing: border-box;
  }